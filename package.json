  "scripts": {
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "build": "yarn clean && yarn build:web && yarn build:server",
    "build:server": "NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json webpack --config ./config/webpack/server.config.ts --env.env=prod",
    "build:web": "NODE_ENV=production TS_NODE_PROJECT=tsconfig.tooling.json webpack --config ./config/webpack/client.config.ts --env.env=prod ",
    "format:js": "prettier '{server,web}/**/*.{js,ts}' --write",
    "lint": "eslint '{server,web}/**/*.{js,ts}'",
    "lint:fix": "yarn eslint --fix",
    "dev": "NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json ts-node ./scripts/dev-server.ts",
    "watch:server": "NODE_ENV=development  tsnd ./server/index.ts",
    "watch:web": "NODE_ENV=development TS_NODE_PROJECT=tsconfig.tooling.json webpack-dev-server --config ./config/webpack/client.config.ts --env.env=dev",
    "postinstall": "typesync"
  },
